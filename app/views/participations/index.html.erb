<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Nationality</th>
      <th scope="col">Sport</th>
      <th scope="col">Editions</th>
      <th scope="col">Number_of_medals</th>
      <th scope="col">Gold</th>
      <th scope="col">Silver</th>
      <th scope="col">Bronze</th>
      <th scope="col">Number</th>
    </tr>
  </thead>
  <tbody>
      <% sum = 0%>
      <% @participations.each do |participation| %>
        <% sum += 1 %>
        <tr>
          <th scope="row"><%= sum %></th>
          <td><%=participation.athlete.name%></td>
          <td><%=participation.edition.name%></td>

          <td><%=participation.sport%></td>

          <td><%=participation.number_of_medals%></td>
          <td><%=participation.gold%></td>
          <td><%=participation.silver%></td>
          <td><%=participation.bronze%></td>
          <td><%#=athlete.number%></td>

        </tr>



      <% end %>
  </tbody>
</table>
