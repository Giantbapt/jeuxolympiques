<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Place</th>
      <th scope="col">Year</th>
      <th scope="col">Slug</th>
      <th scope="col">Logo</th>
      <th scope="col">Season</th>
      <th scope="col">Medals</th>
      <th scope="col">Number_of_medals</th>
    </tr>
  </thead>
  <tbody>
      <% sum = 0%>
      <% @editions.each do |edition| %>

        <tr>
          <th scope="row"><%= sum %></th>
          <td><%=edition.name%></td>
          <td><%=edition.place%></td>
          <td><%=edition.year%></td>
          <td><%=edition.slug%></td>
          <td><%=edition.logo%></td>
          <td><%=edition.season%></td>
          <td><%#=edition.medals%></td>
          <td><%=edition.number_of_athletes%></td>

        </tr>

      <% end %>
  </tbody>
</table>
